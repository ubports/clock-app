#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- -DCLICK_MODE=OFF

override_dh_auto_test:
	echo

override_dh_install:
	dh_install --fail-missing

override_dh_python3:
	dh_python3 -pubuntu-clock-app-autopilot
